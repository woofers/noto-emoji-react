    "@svgr/core": "^5.4.0",
    "@svgr/plugin-svgo": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "node-fetch": "^2.6.0",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "remove": "^0.1.5",
